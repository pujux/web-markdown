{
  "name": "@web-markdown/adapters-next",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./internal": {
      "types": "./dist/internal.d.ts",
      "import": "./dist/internal.js",
      "require": "./dist/internal.cjs"
    },
    "./route-handler": {
      "types": "./dist/route-handler.d.ts",
      "import": "./dist/route-handler.js",
      "require": "./dist/route-handler.cjs"
    },
    "./shared": {
      "types": "./dist/shared.d.ts",
      "import": "./dist/shared.js",
      "require": "./dist/shared.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/internal.ts src/route-handler.ts src/shared.ts --dts --format esm,cjs",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "next": ">=14.2.0"
  },
  "dependencies": {
    "@web-markdown/transform-fetch": "workspace:*"
  },
  "devDependencies": {
    "next": "^16.1.6"
  }
}
